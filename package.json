{
  "name": "inversify-typeorm",
  "version": "0.0.5",
  "description": "Dependency injection and service container integration with TypeORM using InversifyJs library.",
  "author": "felicityin <yinjingping2022@gmail.com>",
  "homepage": "https://github.com/felicityin/inversify-typeorm#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=18"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felicityin/inversify-typeorm.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\" \"__tests__/**/*.ts\" \"examples/**/*.ts\"",
    "format:check": "prettier -cu src/*",
    "format:write": "prettier -wu src/*",
    "clean": "rimraf lib",
    "release": "npm publish --access public.",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bugs": {
    "url": "https://github.com/felicityin/inversify-typeorm/issues"
  },
  "dependencies": {
    "inversify": "6.2.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "typeorm": "0.3.26"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "eslint": "8.29.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "mysql2": "3.14.3",
    "prettier": "2.8.1",
    "rimraf": "6.0.1",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "4.9.4"
  },
  "lint-staged": {
    "**/*": [
      "prettier -wu",
      "npx eslint"
    ]
  }
}
